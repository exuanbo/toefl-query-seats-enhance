{
  "manifest_version": 2,
  "name": "托福考位查询增强",
  "version": "2.0.0",
  "description": "一键查询所选地点所有时间的可预定考位",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": ["https://toefl.neea.cn/myHome/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["https://toefl.neea.cn/myHome/*"],
  "web_accessible_resources": ["app.js"]
}
